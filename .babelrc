{
  "presets": [ "env", ["es2015", {"modules": false}], "stage-2", "react"],
  "plugins": [
    "react-hot-loader/babel",
    "syntax-class-properties",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}
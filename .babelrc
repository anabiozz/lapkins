{
  "presets": ["env", "react", "stage-0"],
  "plugins": [
    "syntax-class-properties",
    "transform-object-rest-spread",
    "babel-plugin-transform-class-properties",
    "transform-decorators-legacy",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}
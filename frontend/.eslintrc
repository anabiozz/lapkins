{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "es6": true,
      "react": true
  },
  "rules": {
      "no-debugger": 0,
      "no-console": 0,
      "new-cap": 0,
      "strict": 1,
      "semi": [2, "never"],
      "no-underscore-dangle": 0,
      "no-use-before-define": 0,
      "eol-last": 0,
      "no-undef": 0
  }
}